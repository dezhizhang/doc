{
  "name": "doc",
  "version": "0.0.2",
  "description": "A static site based on dumi",
  "license": "MIT",
  "scripts": {
    "build": "cross-env REACT_APP_ENV=prod dumi build",
    "dev": "dumi dev",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "dumi": "^2.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "webpack-aliyun-oss-plugin": "^2.1.0"
  },
  "authors": []
}
